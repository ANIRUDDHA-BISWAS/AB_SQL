CREATE DATABASE ab_data;

CREATE TABLE table_1 (
            column_01 varchar(25),
            column_02 varchar(25),
            column_03 varchar(25),
            column_04 varchar(25),
            column_05 varchar(25)
            );
            
INSERT INTO table_1
(column_01, column_02, column_03, column_04, column_05) 
